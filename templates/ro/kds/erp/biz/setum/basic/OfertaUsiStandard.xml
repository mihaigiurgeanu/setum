<?xml version="1.0" encoding="utf-8"?>

<class>
  <package>ro.kds.erp.biz.setum.basic</package>
  <name>OfertaUsiStandard</name>
  <field>
    <name>no</name>
    <type>String</type>
  </field>
  <field>
    <name>docDate</name>
    <type>java.util.Date</type>
  </field>
  <field>
    <name>dateFrom</name>
    <type>java.util.Date</type>
  </field>
  <field>
    <name>dateTo</name>
    <type>java.util.Date</type>
  </field>
  <field>
