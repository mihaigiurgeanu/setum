    <type>String</type>
  </field>
  <field>
    <name>description</name>
    <type>String</type>
  </field>
  <field>
    <name>comment</name>
    <type>String</type>
  </field>
  <field>
    <!--VAT is a constant field filled in by the business logic
    but used in different users scripts -->
    <name>vat</name>
    <type>java.math.BigDecimal</type>
  </field>

  <!-- Subform fields -->
  <field> <!-- The price without VAT -->
    <name>price</name>
    <type>java.math.BigDecimal</type>
  </field>
