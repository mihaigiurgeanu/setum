<?xml version="1.0" encoding="iso-8859-1"?>

<application-client xmlns="http://java.sun.com/xml/ns/j2ee" version="1.4">
  
  <description>Command line client to manage application preferences</description>
  <display-name>prefadmin</display-name>

  <ejb-ref>
    <description>Preferences bean</description>
    <ejb-ref-name>ejb/PreferencesEJB</ejb-ref-name>
    <ejb-ref-type>Session</ejb-ref-type>
    <home>ro.kds.erp.biz.PreferencesHome</home>
    <remote>ro.kds.erp.biz.Preferences</remote>
  </ejb-ref>
  
</application-client>
