	<sql-type>DECIMAL(12,4)</sql-type>
      </cmp-field-jdbc-mapping>
      <cmp-field-jdbc-mapping>
	<field-name>price5</field-name>
	<jdbc-field-name>price5_</jdbc-field-name>
	<sql-type>DECIMAL(12,4)</sql-type>
      </cmp-field-jdbc-mapping>
    </jdbc-mapping>
    <cleanup>create</cleanup>
